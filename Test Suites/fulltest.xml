<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Selenium Tests">

	<listeners>
		<listener class-name="andrewSkye.resources.TestNGListener" />
	</listeners>

	<test thread-count="5" name="Herokuapp Test">
		<classes>
			<class name="andrewSkye.tests.HerokuappTests" />
		</classes>
	</test>

	<test thread-count="5" name="Human Benchmark Test">
		<classes>
			<class name="andrewSkye.tests.HumanBenchmarkTests" />
		</classes>
	</test>

	<test thread-count="5" name="Tutorials Ninja Test">
		<classes>
			<class name="andrewSkye.tests.tutorialsNinja.TutorialsNinjaTests" />
		</classes>
	</test>
</suite>
